@model Adm

<link rel="stylesheet" href="~/css/cad.css">

<body>

<div class="tudo">
    <div class="ret">

        <form asp-controller="Adm" id="form" asp-action="Cadastro" method="post">
            <div class="bg"></div>
            <div class="bg bg2"></div>
            <div class="bg bg3"></div>

            <h1>Cadastre-se</h1>

                <input asp-for="Nome" autocomplete="off" placeholder="Digite o seu nome  " required />
             
           
                <input asp-for="Celular" autocomplete="off" placeholder="Digite o seu celular  "required />
            
             
        
                <input asp-for="email" autocomplete="off" placeholder="Digite o seu email  "required />
             
         
                <input type="password" asp-for="senha" placeholder="Digite a sua senha  " autocomplete="off" required />
          

            <input type="submit" value="Enviar">

        </form>
    </div>

    <div class="retmenor">

    </div>




</div>

</body>